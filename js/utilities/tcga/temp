CREATE TABLE IF NOT EXISTS my_tcga.TCGA_Patient (
  id INT NOT NULL,
  patient_id VARCHAR(45) NULL,
  tissue_source_site VARCHAR(45) NULL,
  bcr_patient_barcode VARCHAR(45) NULL,
  bcr_patient_uuid VARCHAR(65) NULL,
  informed_consent_verified VARCHAR(45) NULL,
  icd_o_3_site VARCHAR(45) NULL,
  icd_o_3_histology VARCHAR(45) NULL,
  icd_10 VARCHAR(45) NULL,
  tissue_prospective_collection_indicator VARCHAR(45) NULL,
  tissue_retrospective_collection_indicator VARCHAR(45) NULL,
  days_to_birth INT NULL,
  days_to_last_known_alive INT NULL,
  gender VARCHAR(15) NULL,
  menopause_status VARCHAR(45) NULL,
  birth_control_pill_history_usage_category VARCHAR(45) NULL,
  prior_tamoxifen_administered_usage_category VARCHAR(45) NULL,
  hypertension VARCHAR(45) NULL,
  days_to_death INT NULL,
  days_to_last_followup INT NULL,
  histological_type VARCHAR(45) NULL,
  bcr_patient_barcode VARCHAR(45) NULL,
  residual_tumor VARCHAR(10) NULL,
  ethnicity VARCHAR(45) NULL,
  days_to_initial_pathologic_diagnosis INT NULL,
  age_at_initial_pathologic_diagnosis INT NULL,
  year_of_initial_pathologic_diagnosis INT NULL,
  person_neoplasm_cancer_status VARCHAR(45) NULL,
  day_of_form_completion INT NULL,
  month_of_form_completion INT NULL,
  year_of_form_completion INT NULL,
  anatomic_neoplasm_subdivision VARCHAR(45) NULL,
  laterality VARCHAR(45) NULL,
  primary_lymph_node_presentation_assessment VARCHAR(45) NULL,
  lymph_node_examined_count VARCHAR(45) NULL,
  number_of_lymphnodes_positive_by_he VARCHAR(45) NULL,
  number_of_lymphnodes_positive_by_ihc VARCHAR(45) NULL,
  lymphovascular_invasion_present VARCHAR(45) NULL,
  perineural_invasion_present VARCHAR(45) NULL,
  neoplasm_histologic_grade VARCHAR(45) NULL,
  year_of_tobacco_smoking_onset VARCHAR(10) NULL,
  stopped_smoking_year VARCHAR(45) NULL,
  number_pack_years_smoked INT NULL,
  alcohol_history_documented VARCHAR(45) NULL,
  frequency_of_alcohol_consumption VARCHAR(45) NULL,
  amount_of_alcohol_consumption_per_day VARCHAR(45) NULL,
  radiation_therapy VARCHAR(45) NULL,
  postoperative_rx_tx VARCHAR(45) NULL,
  primary_therapy_outcome_success VARCHAR(45) NULL,
  other_dx VARCHAR(45) NULL,
  clinical_stage VARCHAR(10) NULL,
  clinical_T VARCHAR(10) NULL,
  clinical_N VARCHAR(10) NULL,
  clinical_M VARCHAR(10) NULL,
  pathologic_T VARCHAR(10) NULL,
  pathologic_N VARCHAR(10) NULL,
  pathologic_M VARCHAR(10) NULL,
  psa_value VARCHAR(45) NULL,
  days_to_psa INT NULL,
  gleason_score VARCHAR(45) NULL,
  primary_pattern VARCHAR(45) NULL,
  secondary_pattern VARCHAR(45) NULL,
  tertiary_pattern VARCHAR(45) NULL,
  PRIMARY KEY (id),
  INDEX bcr_patient_uuid (bcr_patient_uuid ASC),
  INDEX bcr_patient_barcode (bcr_patient_barcode ASC),
  INDEX patient_id (patient_id ASC),
  INDEX icd_o_3_site (icd_o_3_site ASC),
  INDEX icd_o_3_histology (icd_o_3_histology ASC),
  INDEX icd_10 (icd_10 ASC),
  INDEX clinical_stage (clinical_stage ASC),
  INDEX clinical_T (clinical_T ASC),
  INDEX clinical_N (clinical_N ASC),
  INDEX clinical_M (clinical_M ASC),
  INDEX pathologic_T (pathologic_T ASC),
  INDEX pathologic_N (pathologic_N ASC),
  INDEX pathologic_M (pathologic_M ASC))
ENGINE = InnoDB